{
  "name": "@remirror/core",
  "version": "1.0.0-next.56",
  "description": "Where your quest to create a world class editing experience begins.",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/core",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/core",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-core.esm.js",
      "require": "./dist/remirror-core.cjs.js",
      "browser": "./dist/remirror-core.browser.esm.js",
      "types": "./dist/remirror-core.cjs.d.ts",
      "default": "./dist/remirror-core.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-core.cjs.js",
  "module": "dist/remirror-core.esm.js",
  "browser": {
    "./dist/remirror-core.cjs.js": "./dist/remirror-core.browser.cjs.js",
    "./dist/remirror-core.esm.js": "./dist/remirror-core.browser.esm.js"
  },
  "types": "dist/remirror-core.cjs.d.ts",
  "files": [
    "style",
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@linaria/core": "^3.0.0-beta.1",
    "@remirror/core-constants": "1.0.0-next.56",
    "@remirror/core-helpers": "1.0.0-next.56",
    "@remirror/core-types": "1.0.0-next.56",
    "@remirror/core-utils": "1.0.0-next.56",
    "@remirror/i18n": "1.0.0-next.56",
    "@remirror/icons": "0.0.0",
    "@remirror/theme": "1.0.0-next.56",
    "nanoevents": "^5.1.10",
    "tiny-warning": "^1.0.3",
    "type-fest": "^0.20.2"
  },
  "devDependencies": {
    "@remirror/pm": "1.0.0-next.56"
  },
  "peerDependencies": {
    "@remirror/pm": "1.0.0-next.56"
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "40 KB"
  }
