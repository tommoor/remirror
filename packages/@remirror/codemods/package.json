{
  "name": "@remirror/codemods",
  "version": "0.0.0",
  "private": true,
  "description": "Create codemods for breaking changes in remirror releases",
  "keywords": [
    "remirror"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/support/templates/minimal-template",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/codemods",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
