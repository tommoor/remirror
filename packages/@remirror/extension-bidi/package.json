{
  "name": "@remirror/extension-bidi",
  "version": "1.0.0-next.56",
  "description": "Add automatic bidirectional text support to your remirror editor. Just start typing for the magic.",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/extension-bidi",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/extension-bidi",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-bidi.esm.js",
      "require": "./dist/remirror-extension-bidi.cjs.js",
      "browser": "./dist/remirror-extension-bidi.browser.esm.js",
      "types": "./dist/remirror-extension-bidi.cjs.d.ts",
      "default": "./dist/remirror-extension-bidi.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-bidi.cjs.js",
  "module": "dist/remirror-extension-bidi.esm.js",
  "browser": {
    "./dist/remirror-extension-bidi.cjs.js": "./dist/remirror-extension-bidi.browser.cjs.js",
    "./dist/remirror-extension-bidi.esm.js": "./dist/remirror-extension-bidi.browser.esm.js"
  },
  "types": "dist/remirror-extension-bidi.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/direction": "^1.0.0",
    "direction": "^1.0.4"
  },
  "devDependencies": {
    "@remirror/core": "1.0.0-next.56",
    "@remirror/pm": "1.0.0-next.56"
  },
  "peerDependencies": {
    "@remirror/core": "1.0.0-next.56",
    "@remirror/pm": "1.0.0-next.56"
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "5 KB"
  }
}
