{
  "name": "@remirror/extension-font-size",
  "version": "0.0.0",
  "description": "Add font size to the selected text, or text within the provided range.",
  "keywords": [
    "remirror",
    "extension"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/extension-font-size",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/extension-font-size",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-extension-font-size.esm.js",
      "require": "./dist/remirror-extension-font-size.cjs.js",
      "browser": "./dist/remirror-extension-font-size.browser.esm.js",
      "types": "./dist/remirror-extension-font-size.cjs.d.ts",
      "default": "./dist/remirror-extension-font-size.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-extension-font-size.cjs.js",
  "module": "dist/remirror-extension-font-size.esm.js",
  "browser": {
    "./dist/remirror-extension-font-size.cjs.js": "./dist/remirror-extension-font-size.browser.cjs.js",
    "./dist/remirror-extension-font-size.esm.js": "./dist/remirror-extension-font-size.browser.esm.js"
  },
  "types": "dist/remirror-extension-font-size.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  },
  "devDependencies": {
    "@remirror/core": "1.0.0-next.56",
    "@remirror/pm": "1.0.0-next.56"
  },
  "peerDependencies": {
    "@remirror/core": "^1.0.0-next.56",
    "@remirror/pm": "^1.0.0-next.56"
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "5 KB"
  }
}
