{
  "name": "@remirror/core-utils",
  "version": "1.0.0-next.56",
  "description": "Core utilities for dealing with the dom and prosemirror within remirror",
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/core-utils",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/@remirror/core-utils",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/remirror-core-utils.esm.js",
      "require": "./dist/remirror-core-utils.cjs.js",
      "browser": "./dist/remirror-core-utils.browser.esm.js",
      "types": "./dist/remirror-core-utils.cjs.d.ts",
      "default": "./dist/remirror-core-utils.esm.js"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/remirror-core-utils.cjs.js",
  "module": "dist/remirror-core-utils.esm.js",
  "browser": {
    "./dist/remirror-core-utils.cjs.js": "./dist/remirror-core-utils.browser.cjs.js",
    "./dist/remirror-core-utils.esm.js": "./dist/remirror-core-utils.browser.esm.js"
  },
  "types": "dist/remirror-core-utils.cjs.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@remirror/core-constants": "1.0.0-next.56",
    "@remirror/core-helpers": "1.0.0-next.56",
    "@remirror/core-types": "1.0.0-next.56",
    "@types/min-document": "^2.19.0",
    "min-document": "^2.19.0",
    "type-fest": "^0.20.2"
  },
  "devDependencies": {
    "@remirror/pm": "1.0.0-next.56",
    "@types/node": "^14.14.11",
    "domino": "^2.1.6"
  },
  "peerDependencies": {
    "@remirror/pm": "1.0.0-next.56",
    "@types/node": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "meta": {
    "sizeLimit": "25 KB"
  }
}
