{
  "name": "jest-remirror",
  "version": "1.0.0-next.56",
  "description": "A text editor for react built with prosemirror",
  "keywords": [
    "prosemirror",
    "react",
    "ssr",
    "text editor"
  ],
  "homepage": "https://github.com/remirror/remirror/tree/HEAD/packages/jest-remirror",
  "repository": "https://github.com/remirror/remirror/tree/HEAD/packages/jest-remirror",
  "license": "MIT",
  "contributors": [
    "Ifiok Jr. <ifiokotung@gmail.com>"
  ],
  "exports": {
    ".": {
      "require": "./dist/jest-remirror.cjs.js",
      "types": "./dist/jest-remirror.cjs.d.ts"
    },
    "./environment": {
      "require": "./environment/dist/jest-remirror-environment.cjs.js",
      "types": "./environment/dist/jest-remirror-environment.cjs.d.ts"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/declarations/src/*.d.ts"
  },
  "main": "dist/jest-remirror.cjs.js",
  "types": "dist/jest-remirror.cjs.d.ts",
  "files": [
    "dist",
    "environment"
  ],
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@remirror/core": "1.0.0-next.56",
    "@remirror/dom": "1.0.0-next.56",
    "@remirror/pm": "1.0.0-next.56",
    "@remirror/preset-core": "1.0.0-next.56",
    "@testing-library/dom": "^7.28.1",
    "@types/sanitize-html": "^1.27.0",
    "jest-prosemirror": "1.0.0-next.56",
    "jest-snapshot": "^26.6.1",
    "mutationobserver-shim": "^0.3.7",
    "sanitize-html": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.11",
    "jest": "^26.6.3"
  },
  "peerDependencies": {
    "@types/node": "*",
    "jest": ">= 24"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "environment.ts"
    ]
  }
}
